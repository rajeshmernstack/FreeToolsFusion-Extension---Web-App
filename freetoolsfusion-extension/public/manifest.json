{
  "manifest_version": 3,
  "name": "Freetoolsfusion",
  "version": "0.1.0",
  "description": "Fuel Your Online Adventures with FreeToolsFusion, Premium Tools, Priceless Access, For Lifetime!",
  "icons": {
    "16": "icons/icon_16.png",
    "32": "icons/icon_32.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Freetoolsfusion",
    "default_popup": "popup.html"
  },
  "host_permissions": ["http://*/*", "https://*/*"],
  "permissions": [
    "cookies"
  ]
}